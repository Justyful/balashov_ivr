        <activity android:name=".Calculator.CalculatorActivity" />
        <activity
            android:name=".Guide.EditArticleActivity"
            android:noHistory="true" />
        <activity
            android:name=".Guide.NewArticleActivity"
            android:noHistory="true" />
        <activity android:name=".Guide.HistoryActivity" />
        <activity
            android:name=".Guide.ArticleContentActivity"
            android:noHistory="true" />
        <activity android:name=".Guide.ArticlesListActivity" />
        <activity android:name=".Guide.SectionsListActivity" />
        <activity android:name=".MyPhysics" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>